import java.util.*;
import java.io.*;

public class Main {
  public static void main(String args[]) {
    
    //write your code here
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    int Arraysocks[] = new int[t];
    int matchPair[] = new int[t];
    
    for(int i =0 ; i < t;i++){
      Arraysocks[i]=sc.nextInt();
      
      matchPair[i] = Arraysocks[i] + 1;
    }
    
    for(int j = 0; j<t ; j++)
    System.out.println(matchPair[j]);
    
  }
}
