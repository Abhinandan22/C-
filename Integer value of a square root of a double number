import java.io.*;
import java.util.*;
import java.lang.Math;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        
        
        double sqr_root = Math.sqrt(n);
        System.out.println((int)Math.floor(sqr_root));
          
	}
}
