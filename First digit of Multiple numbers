import java.util.*;
import java.io.*;

public class Main {
  public static void main(String args[]) {
    
    //write your code here
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    int ArrayNumber[] = new int[t];
    int DigitNumber[] = new int[t];
    
    
    for(int i=0;i<t;i++){
      
      ArrayNumber[i]=sc.nextInt();
      while(ArrayNumber[i]>= 10){
      ArrayNumber[i]/=10;
      }
      DigitNumber[i]=ArrayNumber[i];
      
     }
   for(int j=0;j<t;j++)
   System.out.println(DigitNumber[j]);
    
    
    
  }
}
