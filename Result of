import java.util.*;
import java.io.*;

public class Main {
  public static void main(String args[]) {
    
    //write your code here
    import java.util.*;
import java.io.*;

public class Main {
  public static void main(String args[]) {
    
    //write your code here
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    
    int count = 0;
    
    while(t-- > 0){
    
      int size = sc.nextInt();
      
      int inputnumber[] = new int[size];
      
      
      for(int y=0 ; y<size; y++){
        inputnumber[y] = sc.nextInt();
      }
      
      
      for(int y=0 ; y<size ;y++){
     
       count = 0;
      if(inputnumber[y] == 0){
      count = count + 1;
      
      }
    
    else {
      System.out.println("hard");
      break;
    }
  }
        if(count == size)
        System.out.println("easy");
    }  
  }
}
